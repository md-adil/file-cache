{
    "include": ["src/**/*.ts"],
    "compilerOptions": {
        "target": "ESNEXT",
        "module": "commonjs",
        "lib": [],
        "outDir": "./dist",
        "strict": true,
        "noImplicitReturns": true,
        "esModuleInterop": true,
        "declaration": true,
        "declarationMap": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
